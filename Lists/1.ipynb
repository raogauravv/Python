      "Enter the fruit 7\n",
      "Orange\n",
      "['Banana', 'Apple', 'Mango', 'Grapes', 'Kiwi', 'Papaya', 'Orange']\n"
     ]
    }
   ],
   "source": [
    "n=7\n",
    "i=1\n",
    "fruits=[]\n",
    "while i<=n:\n",
    "    print(\"Enter the fruit\",i)\n",
    "    a=input()\n",
    "    fruits.append(a)\n",
    "    i+=1\n",
    "print(fruits)"
   ]
